{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
      "build": {
        "dependsOn": ["^db:generate", "^build"],
        "outputs": [".next/**", "!.next/cache/**", "dist/**"]
      },
      "lint": {},
      "dev": {
        "dependsOn": ["^db:generate"],
        "cache": false,
        "persistent": true
      },
      "db:generate": {
        "cache": false
      },
      "db:push": {
        "cache": false
      },
      "db:seed": {},
      "test":{
        "cache": false
      },
      "db:up":{
        "cache": false
      }
    }
  }