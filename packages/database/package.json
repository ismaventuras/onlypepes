{
    "name": "database",
    "version": "0.0.0",
    "scripts": {
        "build": "tsup",
        "db:migrate:deploy": "prisma migrate deploy",
        "db:generate": "prisma generate",
        "db:migrate:dev": "prisma migrate dev",
        "db:seed": "tsx src/seed.ts",
        "db:push": "prisma db push --skip-generate",
        "dev": "npm run build -- --watch",
        "test": "docker-compose up -d && npm run db:push && npm run db:seed && jest && docker-compose down"
    },
    "dependencies": {
        "@prisma/client": "latest"
    },
    "devDependencies": {
        "@types/jest": "^29.5.2",
        "jest": "^29.5.0",
        "prisma": "latest",
        "ts-jest": "^29.1.0",
        "tsconfig": "*",
        "tsup": "latest",
        "tsx": "latest",
        "typescript": "5.1.3"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.js"
}
